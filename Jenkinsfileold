pipeline {
    agent any
    stages {
        stage('nginx-jenkins Build') {
            steps {
                echo 'nginx-jenkins Building..'
            }
        }
        stage('nginx-jenkins Test') {
            steps {
                echo 'nginx-jenkins Testing..'
            }
        }
        stage('nginx-jenkins Deploy') {
            steps {
                echo 'nginx-jenkins Deploying....'
            }
        }
    }
}